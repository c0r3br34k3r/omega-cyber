{
  "name": "@omega/deception-engine",
  "version": "2.0.0",
  "private": true,
  "description": "The Omega Deception Engine: a highly intelligent and autonomous system for generating, deploying, and managing dynamic honeypots and deception layers to misdirect, detect, and analyze adversaries.",
  "license": "AGPL-3.0-or-later",
  "author": "Omega Platform Engineering <contact@omega-cyber.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/omega-cyber/omega.git",
    "directory": "deception-engine"
  },
  "keywords": [
    "omega-platform",
    "cybersecurity",
    "deception",
    "honeypot",
    "ai",
    "microservice",
    "grpc"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "type-check": "tsc --noEmit",
    "generate:proto": "grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./src/proto --js_out=import_style=commonjs,binary:./src/proto --grpc_out=grpc_js:./src/proto -I ../proto ../proto/alert.proto",
    "db:migrate": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
    "db:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/data-source.ts",
    "db:seed": "ts-node src/seeder.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.13",
    "@grpc/proto-loader": "^0.1.0",
    "amqplib": "^0.10.3",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "logform": "^2.6.0",
    "module-alias": "^2.2.3",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "python-shell": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.19",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/config": "^3.3.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.5",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "grpc-tools": "^1.12.4",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}