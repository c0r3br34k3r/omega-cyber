{
  "name": "@omega/command-dashboard",
  "version": "2.0.0",
  "private": true,
  "description": "High-performance, real-time command and control (C2) dashboard for the Omega Cybersecurity Platform. Provides unified observability, operational control, and threat analysis capabilities.",
  "license": "AGPL-3.0-or-later",
  "author": "Omega Platform Engineering <contact@omega-cyber.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/omega-cyber/omega.git",
    "directory": "dashboard"
  },
  "keywords": [
    "omega-platform",
    "cybersecurity",
    "dashboard",
    "c2",
    "real-time",
    "grpc-web",
    "redux-saga",
    "data-visualization"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "vite preview",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:api": "protoc -I=../proto --js_out=import_style=commonjs,binary:./src/gen/proto --grpc-web_out=import_style=typescript,mode=grpcwebtext:./src/gen/proto ../proto/alert.proto",
    "bundle:analyze": "vite build --mode analyze"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@mui/x-data-grid-pro": "^6.18.2",
    "@reduxjs/toolkit": "^1.9.7",
    "ag-grid-community": "^30.2.1",
    "ag-grid-react": "^30.2.1",
    "date-fns": "^2.30.0",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2",
    "framer-motion": "^10.16.5",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.4.2",
    "immer": "^10.0.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.19.0",
    "redux-saga": "^1.2.3",
    "rehype-raw": "^7.0.0",
    "react-markdown": "^9.0.1",
    "reselect": "^5.0.1",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@testing-library/react": "^14.1.2",
    "@types/google-protobuf": "^3.15.10",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^12.10.3",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "storybook": "^7.5.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-visualizer": "^0.6.0",
    "vitest": "^0.34.6"
  }
}
